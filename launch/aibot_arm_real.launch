<launch>

    <param name="robot_description" command="$(find xacro)/xacro.py $(find aibot_description)/urdf/aibot_arm.urdf.xacro" />

    <rosparam file="$(find aibot_description)/config/controllers.yaml" command="load"/>

    <rosparam file="$(find aibot_description)/config/hardware.yaml" command="load" />

    <node pkg="aibot_description" type="aibot_node" name="aibot_node" output="screen"/> 

    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="/" args="
            /aibot/controller/arm_controller
        "/>

    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
        <param name="publish_frequency" type="double" value="100.0" />
    </node>

    <!-- <include file="$(find bricklayer_moveit_config)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include> -->

    <!-- <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher">
        <param name="use_gui" value="true" />
    </node> -->


    <include file="$(find aibot_arm_moveit_config)/launch/move_group.launch" >
        <arg name="allow_trajectory_execution" value="true"/>
        <arg name="fake_execution" value="false"/>
        <arg name="info" value="true"/>
        <param name="allowed_execution_duration_scaling" value="5"/>
        <param name="allowed_goal_duration_margin" value="5"/>
        <param name="execution_duration_monitoring" value="false"/>
        <arg name="publish_monitored_planning_scene" value="true" />
    </include>

</launch>

